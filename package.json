{
  "name": "capsulib",
  "version": "1.0.0",
  "description": "Capsule wardrobe management application",
  "scripts": {
    "start-backend": "cd backend && uvicorn main:app --reload",
    "start-frontend": "cd frontend && npm start",
    "install-frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "keywords": [
    "capsule",
    "wardrobe",
    "fashion"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SytzeSimonse/capsulib.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SytzeSimonse/capsulib/issues"
  },
  "homepage": "https://github.com/SytzeSimonse/capsulib#readme",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}